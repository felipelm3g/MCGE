
# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:login.eti.br
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} ^login.eti.br$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE SSL_REDIRECT:login.eti.br
ErrorDocument 404 /404
